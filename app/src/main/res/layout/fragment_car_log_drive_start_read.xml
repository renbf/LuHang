<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.ContentFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">


    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:background="@color/color_window" />

            <!--填报日期-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="填报日期"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_fill_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:gravity="center_vertical|end"
                    android:textSize="@dimen/sp_14" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--车辆好牌-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="车辆号牌"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_car_plate"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--核载吨位-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="核载吨位"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_car_load_nuclear"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:inputType="numberDecimal"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--实载吨位-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="实载吨位"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_car_load_actual"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:inputType="numberDecimal"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--货物名称-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="货物名称"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_goods_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:id="@+id/goods_type_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/margin_start_end"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/color_window" />
                <!--类项-->
                <android.support.v7.widget.LinearLayoutCompat
                    style="@style/padding_start_end"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatTextView
                        android:layout_width="@dimen/dp_100"
                        android:layout_height="wrap_content"
                        android:drawableStart="@mipmap/ic_bi_tian"
                        android:drawablePadding="@dimen/dp_3"
                        android:text="类项"
                        android:textSize="@dimen/sp_16"
                        tools:ignore="HardcodedText" />

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/tv_start_goods_type"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/dp_15"
                        android:background="@null"
                        android:gravity="center_vertical|end"
                        android:singleLine="true"
                        android:textSize="@dimen/sp_14"
                        tools:ignore="HardcodedText" />
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--驾驶员姓名-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="驾驶员姓名1"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_drive_first"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:text="驾驶员姓名2"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_drive_second"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:id="@+id/escort_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/margin_start_end"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/color_window" />
                <!--押运员姓名-->
                <android.support.v7.widget.LinearLayoutCompat
                    style="@style/padding_start_end"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatTextView
                        android:layout_width="@dimen/dp_100"
                        android:layout_height="wrap_content"
                        android:drawableStart="@mipmap/ic_bi_tian"
                        android:drawablePadding="@dimen/dp_3"
                        android:text="押运员姓名"
                        android:textSize="@dimen/sp_16"
                        tools:ignore="HardcodedText" />

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/tv_start_escort_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/dp_15"
                        android:background="@null"
                        android:gravity="center_vertical|end"
                        android:singleLine="true"
                        android:textSize="@dimen/sp_14"
                        tools:ignore="HardcodedText" />
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:background="@color/color_window" />
            <!--气温-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:text="气温"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_temperature"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />

            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_fine"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:button="@null"
                    android:drawableStart="@drawable/selector_check_box_s"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center"
                    android:text="晴"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_yin"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:button="@null"
                    android:drawableStart="@drawable/selector_check_box_s"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center"
                    android:text="阴"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_rain"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:button="@null"
                    android:drawableStart="@drawable/selector_check_box_s"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center"
                    android:text="雨"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_fog"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:button="@null"
                    android:drawableStart="@drawable/selector_check_box_s"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center"
                    android:text="雾"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_snow"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:button="@null"
                    android:drawableStart="@drawable/selector_check_box_s"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center"
                    android:text="雪"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_frozen"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:button="@null"
                    android:drawableStart="@drawable/selector_check_box_s"
                    android:drawablePadding="@dimen/dp_5"
                    android:gravity="center"
                    android:text="冰冻"
                    tools:ignore="HardcodedText" />

            </android.support.v7.widget.LinearLayoutCompat>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:background="@color/color_window" />
            <!--启运日期-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="启运日期"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_shipment_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:gravity="center_vertical|end"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--启运地-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="启运地"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_shipment_location"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--目的地-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="目的地"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_destination"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--里程（公里）-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:text="里程（公里）"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_mileage"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--到达日期-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="到达日期"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_at_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:gravity="center_vertical|end"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--行车前安全员车辆检查事项-->
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <android.support.v7.widget.AppCompatTextView
                    style="@style/margin_start_end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="行车前安全员车辆检查事项"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <com.zhy.view.flowlayout.TagFlowLayout
                    android:id="@+id/start_check_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5" />

                <View
                    style="@style/margin_start_end"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_15"
                    android:background="@color/color_window" />
            </android.support.v7.widget.LinearLayoutCompat>

            <!--不符合项-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:text="不符合项"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_not_meet"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />

            <!--确认结论-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:text="确认结论"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_drive_conclusion"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:singleLine="true"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--备注-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/padding_start_end"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="@dimen/dp_15"
                android:paddingBottom="@dimen/dp_15">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="备注"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_remarks"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:background="@null"
                    android:gravity="end|center_vertical"
                    android:lineSpacingExtra="@dimen/dp_3"
                    android:textSize="@dimen/sp_14"
                    tools:ignore="HardcodedText" />
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />

            <!--行车照片-->
            <android.support.v7.widget.LinearLayoutCompat
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/dp_15"
                android:paddingBottom="@dimen/dp_15">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="行车照片"
                    android:textSize="@dimen/sp_16"
                    tools:ignore="HardcodedText" />

                <com.gfc.library.widget.grid.ScrollGridView
                    android:id="@+id/start_car_grid_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:horizontalSpacing="@dimen/dp_15"
                    android:numColumns="2"
                    android:verticalSpacing="@dimen/dp_15">

                </com.gfc.library.widget.grid.ScrollGridView>
            </android.support.v7.widget.LinearLayoutCompat>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_window" />
            <!--签名-->
            <android.support.constraint.ConstraintLayout
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/dp_15"
                android:paddingBottom="@dimen/dp_15">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_start_sign"
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:drawableStart="@mipmap/ic_bi_tian"
                    android:drawablePadding="@dimen/dp_3"
                    android:gravity="center_vertical"
                    android:text="签名"
                    android:textSize="@dimen/sp_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="HardcodedText" />

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/iv_start_sign"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/dp_15"
                    android:scaleType="fitCenter"
                    android:src="@mipmap/ic_user_sign"
                    app:layout_constraintDimensionRatio="h,215:120"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/tv_start_sign"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <View
                style="@style/margin_start_end"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginBottom="@dimen/dp_15"
                android:background="@color/color_window" />
        </android.support.v7.widget.LinearLayoutCompat>
    </android.support.v4.widget.NestedScrollView>

    <include layout="@layout/lib_loading_layout" />
</android.support.v7.widget.ContentFrameLayout>